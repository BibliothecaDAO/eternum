{
"version": 2,
"rewrites": [
    {
    "source": "/mobile",
    "destination": "/mobile/index.html"
    },
    {
    "source": "/mobile/(.*)",
    "destination": "/mobile/index.html"
    },
    {
    "source": "/trade/:collection*",
    "destination": "/index.html"
    },
    {
    "source": "/(.*)",
    "destination": "/index.html"
    }
]
}
