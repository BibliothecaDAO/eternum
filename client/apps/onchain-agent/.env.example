# Chain to connect to (slot, slottest, sepolia, mainnet, local)
# CHAIN=slot

# Optional overrides (auto-resolved from factory if not set)
# RPC_URL=
# TORII_URL=
# WORLD_ADDRESS=
# MANIFEST_PATH=       # Only needed if bypassing world discovery

# Optional base directory for local runtime files
ETERNUM_AGENT_HOME=
# Optional explicit data directory override (defaults to $ETERNUM_AGENT_HOME/data)
DATA_DIR=

# Cartridge Controller session
SESSION_BASE_PATH=.cartridge
GAME_NAME=eternum

# LLM provider (see @mariozechner/pi-ai for supported providers)
# Anthropic
ANTHROPIC_API_KEY=

# OpenAI
OPENAI_API_KEY=

# Agent settings
TICK_INTERVAL_MS=60000
LOOP_ENABLED=true
MODEL_PROVIDER=anthropic
MODEL_ID=claude-sonnet-4-5-20250929
