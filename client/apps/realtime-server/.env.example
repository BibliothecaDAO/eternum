DATABASE_URL=postgres://user:password@localhost:5432/database_name

# Avatar generation settings
FAL_KEY=your_fal_api_key_here
AVATAR_MAX_GENERATIONS_PER_WEEK=1
AVATAR_STORAGE_TYPE=fal

# Optional: CORS origins (comma-separated)
# CORS_ORIGIN=http://localhost:3000,http://localhost:5173

# Torii SQL endpoint for cached Torii SQL queries
# TORII_SQL_BASE_URL=http://localhost:8080/sql

# Optional: Cache tuning (milliseconds, counts)
# CACHE_ENABLED=true
# LEADERBOARD_CACHE_TTL_MS=60000
# LEADERBOARD_CACHE_STALE_MS=300000
# LEADERBOARD_CACHE_MAX_ENTRIES=5
# LEADERBOARD_CACHE_LIMIT_DEFAULT=50
# LEADERBOARD_CACHE_LIMIT_MAX=5000
# STORY_EVENTS_CACHE_TTL_MS=10000
# STORY_EVENTS_CACHE_STALE_MS=120000
# STORY_EVENTS_CACHE_MAX_ENTRIES=25
# STORY_EVENTS_CACHE_LIMIT_DEFAULT=50
# STORY_EVENTS_CACHE_LIMIT_MAX=2000
# TILES_CACHE_TTL_MS=5000
# TILES_CACHE_STALE_MS=30000
# TILES_CACHE_MAX_ENTRIES=2
# HYPERSTRUCTURES_CACHE_TTL_MS=5000
# HYPERSTRUCTURES_CACHE_STALE_MS=30000
# HYPERSTRUCTURES_CACHE_MAX_ENTRIES=10
# STRUCTURE_EXPLORER_DETAILS_CACHE_TTL_MS=5000
# STRUCTURE_EXPLORER_DETAILS_CACHE_STALE_MS=30000
# STRUCTURE_EXPLORER_DETAILS_CACHE_MAX_ENTRIES=5

# Optional: Server port
# PORT=8080
